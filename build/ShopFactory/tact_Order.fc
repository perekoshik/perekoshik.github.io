builder $Order$_store(builder build_0, (slice, slice, slice, int, int, slice) v) inline {
    var (v'seller, v'buyer, v'itemAddress, v'id, v'completed, v'deliveryAddress) = v;
    build_0 = build_0.store_slice(v'seller);
    build_0 = build_0.store_slice(v'buyer);
