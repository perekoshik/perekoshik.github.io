(slice, slice, slice, int, int, slice) $Order$_contract_load() impure inline {
    slice $sc = get_data().begin_parse();
    int $loaded = $sc~load_int(1);
    if ($loaded) {
        return $sc~$Order$_load();
    }
    else {
        (slice $seller, slice $buyer, slice $itemAddress, int $id, slice $deliveryAddress) = $sc~$Order$init$_load();
        $sc.end_parse();
        return $Order$_contract_init($seller, $buyer, $itemAddress, $id, $deliveryAddress);
    }
}

() $Order$_contract_store((slice, slice, slice, int, int, slice) v) impure inline {
    builder b = begin_cell();
    b = b.store_int(true, 1);
    b = $Order$_store(b, v);
    set_data(b.end_cell());
}

